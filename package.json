{
  "name": "process-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run build:css && concurrently -k \"next dev\"",
    "build": "npm run build:css && concurrently -k \"next build\"",
    "start": "next start",
    "prepare": "husky",
    "lint": "next lint && eslint --ext .js,.ts,.tsx .",
    "type-check": "npx tsc",
    "test": "node --import tsx --test ./src/**/*.test.*",
    "test:watch": "node --import tsx --test --watch ./src/**/*.test.*",
    "build:css": "npx postcss src/styles/dark.css src/styles/light.css -d public/theme"
  },
  "dependencies": {
    "@trpc/client": "^10.35.0",
    "@trpc/next": "^10.35.0",
    "@trpc/react-query": "^10.35.0",
    "@trpc/server": "^10.35.0",
    "classnames": "^2.5.1",
    "concurrently": "^8.2.2",
    "easy-typed-intl": "^1.0.3",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.2",
    "typescript": "^5",
    "typescript-plugin-css-modules": "^5.1.0"
